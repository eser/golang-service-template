{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch identitysvc",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "cwd": "${workspaceFolder}",
      "program": "cmd/identitysvc/"
    },
    {
      "name": "Launch http-webclient",
      "type": "node-terminal",
      "request": "launch",
      "cwd": "${workspaceFolder}/pkg/web",
      "command": "deno run --inspect --allow-all dev",
      "skipFiles": [
        "<node_internals>/**"
      ]
    }
  ]
}
